{
  "name": "gene-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Standalone desktop ver. of Fogg Lab's Gene Platform",
  "main": "main.js",
  "homepage": "https://fogg-lab.github.io/gene-platform",
  "scripts": {
    "start:web": "craco start",
    "build:web": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "start:electron": "webpack serve --config webpack.config.js --mode development --stats-children",
    "build:electron": "webpack --config webpack.config.js --mode production",
    "electron": "electron .",
    "predeploy": "npm run build:web",
    "deploy": "gh-pages -d build",
    "start": "npm run start:electron",
    "build": "npm run build:electron",
    "dev": "concurrently \"npm run start\" \"wait-on http://localhost:9328 && npm run electron\"",
    "prod": "npm run build && electron ."
  },
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.25.9",
    "@emotion/styled": "^11.14.0",
    "@mui/x-data-grid-pro": "^7.23.1",
    "assert": "^2.1.0",
    "browser": "^0.2.6",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.1",
    "gsea-rs": "https://github.com/fogg-lab/gsea-rs/releases/download/latest/gsea-rs.tar.gz",
    "pako": "^2.1.0",
    "papaparse": "^5.4.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "pyodide": "^0.26.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^7.0.2",
    "react-scripts": "^5.0.1",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@pyodide/webpack-plugin": "^1.3.3",
    "babel-loader": "^9.2.1",
    "browserify": "^17.0.1",
    "concurrently": "^9.1.0",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "electron": "^33.2.1",
    "electron-extension-installer": "^1.2.0",
    "electron-is-dev": "^3.0.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.2.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.4.49",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.16",
    "wait-on": "^8.0.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^5.1.4",
    "worker-loader": "^3.0.8",
    "worker-plugin": "^5.0.1"
  }
}
