{
  "name": "gene-platform",
  "version": "1.0.0",
  "private": true,
  "description": "Standalone desktop ver. of Fogg Lab's Gene Platform",
  "main": "main.js",
  "scripts": {
    "start:web": "react-app-rewired start",
    "build:web": "react-app-rewired build",
    "start:electron": "webpack serve --config webpack.config.js --mode development",
    "build:electron": "webpack --config webpack.config.js --mode production",
    "electron": "electron .",
    "predeploy": "npm run build:web",
    "deploy": "gh-pages -d build",
    "start": "npm run start:electron",
    "build": "npm run build:electron",
    "dev": "concurrently \"npm run start\" \"wait-on http://localhost:9328 && npm run electron\"",
    "prod": "npm run build && electron ."
  },
  "homepage": "https://fogg-lab.github.io/gene-platform",
  "devDependencies": {
    "@pyodide/webpack-plugin": "^1.3.2",
    "babel-loader": "^9.1.3",
    "browserify": "^17.0.0",
    "concurrently": "^8.2.2",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "electron": "^32.0.1",
    "electron-extension-installer": "^1.2.0",
    "electron-is-dev": "^3.0.1",
    "file-loader": "^6.2.0",
    "gh-pages": "^6.1.1",
    "html-webpack-plugin": "^5.6.0",
    "postcss": "^8.4.31",
    "react-app-rewired": "^2.2.1",
    "tailwindcss": "^3.4.1",
    "wait-on": "^8.0.0",
    "webpack": "^5.94.0",
    "worker-loader": "^3.0.8",
    "worker-plugin": "^5.0.1"
  },
  "dependencies": {
    "@mui/x-data-grid-pro": "^7.22.1",
    "assert": "^2.1.0",
    "browser": "^0.2.6",
    "buffer": "^6.0.3",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.1",
    "gsea-rs": "https://github.com/fogg-lab/gsea-rs/releases/download/latest/gsea-rs.tar.gz",
    "pako": "^2.1.0",
    "papaparse": "^5.4.1",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "pyodide": "^0.26.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^6.28.0",
    "stream-browserify": "^3.0.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}